{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "cf-ai-interview-agent",
	"main": "src/index.ts",
	"compatibility_date": "2025-09-22",
	"observability": {
		"enabled": true
	},
	"durable_objects": {
		"bindings": [
			{
				"name": "INTERVIEW_SESSION",
				"class_name": "InterviewSession"
			}
		]
	},
	"ai": {
		"binding": "AI"
	},
	"migrations": [
		{
			"tag": "v1",
			"new_sqlite_classes": [
				"InterviewSession"
			]
		}
	],
	"assets": {
		"binding": "ASSETS",
		"directory": "./public"
	}
}

